using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace swiiming
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int n = 5;
            for(int i = 0; i < n; i++)
            {
                for(int j = i+1; j < n; j++)
                {
                    Console.Write(" ");
                }
                for(int k = 0; k < i+1; k++)
                {
                    Console.Write("*");
                }
                Console.WriteLine();
            }
        }
    }

}

