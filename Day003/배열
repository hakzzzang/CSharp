using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ArrayApp002
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int[] arr = new int[100];

            //입력
            for(int i=0; i<arr.Length; i++)
            {
                arr[i] = i+1;
                Console.Write(arr[i] + " ");
            }
        }
    }
}
