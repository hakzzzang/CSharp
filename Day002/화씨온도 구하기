using System;

using System.Collections.Generic;

using System.Linq;

using System.Text;

using System.Threading.Tasks;



namespace MyApp007

{

    internal class Program

    {

        static void Main(string[] args)

        {

            Console.WriteLine("1 에서 100 사이의 자연수를 입력해주세요.");

            int a = Int32.Parse(Console.ReadLine());

            double result;

            result = (double)9 / 5 * a + 32;

            Console.WriteLine($"화씨 온도 : {result}");

        }

    }

}
